	"authors": [
		"PandaBear29",
		"NotEnoughUpdate Contributors"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/afranz29/Inventory-Buttons-1.21.5"
	},
